<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 5.3//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">
<hibernate-mapping>
    <class name="com.caseStudy.bank.Account" table="bank"> <!-- table name -->
        <id name="accountNo"> <!-- Primary key -->
            <generator class="native"></generator> <!-- Auto generated values -->
        </id> 
        <property name="name"></property> <!-- column names -->
        <property name="balance"></property> <!-- column names -->
        <property name="activateDate"></property> <!-- column names -->
         <property name="pin"></property> <!-- column names -->
          <property name="isActive"></property> <!-- column names -->
           <property name="privilage"></property> <!-- column names -->
    </class>
</hibernate-mapping>